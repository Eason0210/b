ap() {
  awk "{print \$$1}"
}

eval $(cat "${SSHHOME}/.sshrc.d/aliases" \
  | grep -v '^#' \
  | grep -vE '^\s*$' \
  | sed 's/\$/\\$/' \
  | sed 's/^\([^ :]*\)[[:space:]]*:\(.*\)/alias \1="\2";/')

source "${SSHHOME}/.sshrc.d/functions.sh"

eval $(dircolors -b "${SSHHOME}/.sshrc.d/LS_COLORS")
alias ls="ls --color=auto -F"


